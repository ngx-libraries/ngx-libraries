    "@ngx-library/zip-js": {
      "projectType": "library",
      "root": "projects/zip-js",
      "sourceRoot": "projects/zip-js",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
